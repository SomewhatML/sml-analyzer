{
	"name": "sml-analyzer-client",
	"description": "VSCode extension for sml-analyzer",
	"author": "Michael Lazear",
	"license": "MIT",
	"version": "0.0.1",
	"publisher": "vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/lazear/sml-analyzer"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"activationEvents": [
		"onLanguage:sml"
	],
	"main": "./out/extension",
	"dependencies": {
		"vscode-languageclient": "^6.1.3"
	},
	"contributes": {
		"languages": [
			{
				"id": "sml",
				"extensions": [
					".sml",
					".fun",
					".sig"
				],
				"aliases": [
					"Standard ML",
					"sml"
				]
			}
		]
	},
	"devDependencies": {
		"@types/vscode": "1.43.0",
		"vscode-test": "^1.3.0",
		"@types/mocha": "^7.0.2",
		"mocha": "^8.0.1",
		"@types/node": "^12.12.0",
		"eslint": "^6.4.0",
		"@typescript-eslint/parser": "^2.3.0",
		"typescript": "^3.9.4"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w"
	}
}
